{
  "name": "Fuze_Connect",
  "description": "Fuze Connect Installable",
  "version": "2.0.1",
  "target": "electron",
  "author": {
    "name": "Fuze Solution Engineering",
    "email": "coe@fuze.com",
    "url": "https://www.fuze.com/"
  },
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "https://github.com/fuze/coe-electron-fuze-connect.git"
  },
  "main": "./src/fuzeApp.js",
  "scripts": {
    "test": "standard && mocha --recursive",
    "unit": "standard && mocha test/unit --recursive",
    "integration": "standard && mocha test/integration --recursive",
    "coverage": "istanbul cover node_modules/.bin/_mocha -- --recursive",
    "depcheck": "dependency-check app --entry fuzeApp.js --unused --no-dev",
    "start": "electron ./src/fuzeApp",
    "clean:linux": "rimraf dist/linux* dist/*.deb dist/*.AppImage",
    "clean:osx": "rimraf dist/mac",
    "clean:win": "rimraf dist/win*",
    "clean": "npm run clean:linux && npm run clean:win && npm run clean:osx",
    "pack:osx": "build --dir --mac",
    "pack:win": "build --dir --win --x64",
    "pack:linux": "build --dir --linux --x64",
    "pack": "build --dir",
    "prebuild": "node build/target.js",
    "build:osx": "npm run prebuild && build --mac && npm run postbuild",
    "build:win": "npm run prebuild && build --win --x64 && npm run postbuild",
    "build:linux": "npm run prebuild && build --linux deb AppImage --x64 && npm run postbuild",
    "build": "build",
    "postbuild": "node build/target.js --clean",
    "builder-win-local": "electron-builder -w --x64",
    "builder-win": "electron-builder -w --x64 -p always",
    "builder-mac-local": "electron-builder --mac",
    "builder-mac": "electron-builder --mac -p always",
    "dist": "build"
  },
  "standard": {
    "globals": [
      "describe",
      "after",
      "it",
      "before",
      "beforeEach",
      "afterEach",
      "rm",
      "cp"
    ]
  },
  "pre-commit": [],
  "keywords": [
    "electron-app",
    "electron",
    "boilerplate"
  ],
  "build": {
    "appId": "com.soleng-fuze-connect-electron",
    "productName": "Fuze Connect",
    "asar": true,
    "compression": "maximum",
    "win": {
      "target": [
        "squirrel"
      ]
    },
    "mac": {
      "target": [
        "dir",
        "dmg"
      ]
    },
    "nsis": {
      "oneClick": false,
      "allowElevation": true,
      "runAfterFinish": false
    },
    "publish": {
      "repo": "soleng-fuze-connect-electron",
      "owner": "fuze",
      "private": true,
      "provider": "github",
      "token": "512ee5fdf14c0afee7a46904fbc45477ddf1b702"

    },

    "mac": {
      "icon": "src/assets/icons/mac/icon.icns"
    },
    "win": {
      "icon": "src/assets/icons/win/icon.ico"
    },
    "linux": {
      "icon": "build/icons"
    }
  },
  "devDependencies": {
    "async": "^2",
    "chai": "^3",
    "chai-as-promised": "^6",
    "dependency-check": "^2",
    "devtron": "^1.4.0",
    "electron-builder-squirrel-windows": "^19.45.0",
    "electron-debug": "^1",
    "electron-eventbus": "^1",
    "electron-is-dev": "^0.1",
    "electron": "^1.7.12",
    "electron-settings": "^3",
    "electron-window-state": "^3",
    "events": "^1",
    "google-libphonenumber": "^3",
    "istanbul": "^0.4",
    "libphonenumber-node": "^0",
    "mocha": "^3",
    "moment": "^2.19.1",
    "pre-commit": "^1",
    "rimraf": "^2",
    "shelljs": "^0.7",
    "spectron": "^3",
    "standard": "^8.6.0",
    "xmlhttprequest": "^1"
  },
  "dependencies": {
    "async": "^2",
    "chai": "^3",
    "chai-as-promised": "^6",
    "electron-debug": "^1",
    "electron-eventbus": "^1",
    "electron-is-dev": "^0.1",
    "electron-settings": "^3",
    "electron-window-state": "^3",
    "events": "^1",
    "google-libphonenumber": "^3",
    "istanbul": "^0.4",
    "libphonenumber-node": "^0",
    "mocha": "^3",
    "moment": "^2.19.1",
    "pre-commit": "^1",
    "rimraf": "^2",
    "shelljs": "^0.7",
    "spectron": "^3",
    "standard": "^8.6.0",
    "xmlhttprequest": "^1"
  }
}
